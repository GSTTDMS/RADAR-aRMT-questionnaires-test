[
  {
    "field_label": "Every time you decide to take your biologic injection (risankizumab) during the study please record this in the injection log. The interval between each injection must be <b><u>12 weeks or longer</u></b>.",
    "field_type": "descriptive",
    "field_name": "t1_txt"
  },
  {
    "field_label": "When did your psoriasis start to recur?",
    "field_type": "text",
    "field_name": "t1_precur",
    "required_field": "y",
    "text_validation_type_or_show_slider_number": "date_dmy",
    "field_annotation": "@NOTFUTURE"
  },
  {
    "field_label": "When did you take your biologic injection?",
    "field_type": "text",
    "field_name": "t1_injdte",
    "required_field": "y",
    "text_validation_type_or_show_slider_number": "date_dmy",
    "field_annotation": "@NOTFUTURE"
  },
  {
    "field_label": "[716_0]",
    "field_type": "calc",
    "field_name": "t1_injdte_calc",
    "calculation_fn": "return new Date((new Date(t1_injdte+'Z').getTime())).getTime()",
    "calculation_args": "['t1_injdte']"
  },
  {
    "field_label": "[716_1]",
    "field_type": "calc",
    "field_name": "t1_injdtenxt_calc",
    "calculation_fn": "return new Date((new Date(t1_injdte+'Z').getTime() + 4*60*1000)).getTime()",
    "calculation_args": "['t1_injdte']"
  },
  {
    "field_label": "",
    "field_type": "calc",
    "field_name": "t1_injdtenxt_str_calc",
    "calculation_fn": "var options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric', hour: 'numeric', minute: 'numeric'};return new Date(+t1_injdtenxt_calc).toLocaleString(undefined, options);",
    "calculation_args": "['t1_injdtenxt_calc']"
  },
  {
    "field_label": "When you took your biologic injection, was your psoriasis:",
    "field_type": "radio",
    "field_name": "t1_psoriasis",
    "required_field": "y",
    "select_choices_or_calculations": [
      {
        "code": "1",
        "label": "1 - Clear"
      },
      {
        "code": "2",
        "label": "2 - Nearly clear"
      },
      {
        "code": "3",
        "label": "3 - Mild"
      },
      {
        "code": "4",
        "label": "4 - Moderate"
      },
      {
        "code": "5",
        "label": "5 - Moderate-severe"
      },
      {
        "code": "6",
        "label": "6 - Severe"
      }
    ]
  },
  {
    "field_label": "Why did you take your biologic injection?",
    "field_type": "radio",
    "field_name": "t1_injwhy",
    "required_field": "y",
    "select_choices_or_calculations": [
      {
        "code": "1",
        "label": "I could see my psoriasis recurring"
      },
      {
        "code": "2",
        "label": "I could feel (but not see) my psoriasis recurring (e.g. itch)"
      },
      {
        "code": "3",
        "label": "I developed joint pains"
      },
      {
        "code": "4",
        "label": "Other (please specify)"
      }
    ]
  },
  {
    "field_label": "Please specify other reason why you took your biologic injection?",
    "field_type": "text",
    "field_name": "t1_injwhyoth",
    "required_field": "y",
    "branching_logic": "[t1_injwhy]='4'"
  },
  {
    "field_label": "After you took your injection, did you develop any problems at the site of your injection?",
    "field_type": "radio",
    "field_name": "t1_injprb",
    "required_field": "y",
    "select_choices_or_calculations": [
      {
        "code": "0",
        "label": "No"
      },
      {
        "code": "1",
        "label": "Yes"
      }
    ]
  },
  {
    "field_label": "Please specify problems you developed at the site of your injection? Tick all that apply.",
    "field_type": "checkbox",
    "field_name": "t1_injprbyes",
    "required_field": "y",
    "select_choices_or_calculations": [
      {
        "code": "1",
        "label": "Redness"
      },
      {
        "code": "2",
        "label": "Swelling"
      },
      {
        "code": "3",
        "label": "Pain"
      },
      {
        "code": "4",
        "label": "Itch"
      },
      {
        "code": "5",
        "label": "Other (please specify)"
      }
    ]
  },
  {
    "field_label": "Please specify other problems you developed at the site of your injection?",
    "field_type": "text",
    "field_name": "t1_injprbyesoth",
    "required_field": "y",
    "branching_logic": "[t1_injprbyes]='5'"
  },
  {
    "field_label": "Do you want to upload photo?",
    "field_type": "radio",
    "field_name": "photo_have",
    "required_field": "y",
    "section_header": "Psoriasis",
    "select_choices_or_calculations": [
      {
        "code": "0",
        "label": "No"
      },
      {
        "code": "1",
        "label": "Yes"
      }
    ]
  },
  {
    "field_name": "photo_worst",
    "section_header": "Uploading your photographs",
    "field_type": "photo",
    "field_label": "Upload the photograph of your worst affected site by clicking on the green 'upload file' link and select the best photo of your worst affected site from your photo library.",
    "required_field": "true",
    "branching_logic": "[photo_have='1']"
  },
  {
    "field_label": "The earliest date that you can take your next injection is: [t1_injdtenxt_str_calc]",
    "field_type": "descriptive",
    "field_name": "t1_injdtenxt",
    "section_header": "Uploading your photographs"
  },
  {
    "field_label": "Do you have any other comments?",
    "field_type": "notes",
    "field_name": "t1_cmt",
    "required_field": "false",
    "section_header": "Uploading your photographs"
  }
]
